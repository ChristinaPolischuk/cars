mixin carsFiltersItem(folder, src, text)
  .cars-filters__item 
    label.checkbox
      input(type="checkbox")
      span.checkbox__box 
        i
      span.checkbox__text=text
    picture
      source(srcset=`img/content/${folder}/${src}.avif`, type="image/avif")
      source(srcset=`img/content/${folder}/${src}.webp`, type="image/webp")
      img.cars-filters__img(src=`img/content/${folder}/${src}.png`, alt=text)

section.section-middle
  .container
    .filters
      .filters-header
        h1.car-model__title Search RAV4 Inventory
        .filters-box
          .filters-box__group
            label.filters-box__label Search Radius:
            .select__wrap
              +icon("location", "select-icon-location")
              +icon("caret-down", "select-icon-caret-down")
              select.select.select2
                option Any
                option 10 Miles
                option 25 Miles
                option 50 Miles
                option 75 Miles
                option 100 Miles
                option 150 Miles
                option 250 Miles
                option 500 Miles
          .filters-box__group
            label.filters-box__label Zip Code:
            input.field(type="number")
      .tabs 
        .tabs-nav 
          button.tab-nav__btn.btn-dropdown.js-tab-nav(data-tab="#vehicles") Vehicles
          button.tab-nav__btn.btn-dropdown.js-tab-nav(data-tab="#price") Price
          button.tab-nav__btn.btn-dropdown.js-tab-nav(data-tab="#year") Year
          button.tab-nav__btn.btn-dropdown.js-tab-nav(data-tab="#mileage") Mileage
          button.tab-nav__btn.btn-dropdown.js-tab-nav(data-tab="#color") Color
          button.tab-nav__btn.btn-dropdown.js-tab-nav(data-tab="#drivetrain") Drivetrain
          button.tab-nav__btn.btn-dropdown.js-tab-nav(data-tab="#dealer") Dealer
        .tabs-content
          .tab-content.js-tab-content#vehicles
            .subtabs
              .subtabs-nav 
                button.subtab-nav__btn.active.js-subtab-nav(data-tab="#cars-and-minivan") Cars &amp; Minivan
                button.subtab-nav__btn.js-subtab-nav(data-tab="#trucks") Trucks
                button.subtab-nav__btn.js-subtab-nav(data-tab="#crossovers-and-suvs") Crossovers &amp; SUVs
                button.subtab-nav__btn.js-subtab-nav(data-tab="#electrified") Electrified
                button.subtab-nav__btn.js-subtab-nav(data-tab="#scion-by-toyota") Scion by Toyota
              .subtabs-content
                .subtab-content.active.js-subtab-content#cars-and-minivan
                  .cars-filters 
                    +carsFiltersItem("cars-and-minivan", "86", "86")
                    +carsFiltersItem("cars-and-minivan", "avalon", "Avalon")
                    +carsFiltersItem("cars-and-minivan", "camry", "Camry")
                    +carsFiltersItem("cars-and-minivan", "corolla", "Corolla")
                    +carsFiltersItem("cars-and-minivan", "corollahatchback", "Corolla Hatchback")
                    +carsFiltersItem("cars-and-minivan", "corollaim", "Corolla iM")
                    +carsFiltersItem("cars-and-minivan", "grcorolla", "GR Corolla")
                    +carsFiltersItem("cars-and-minivan", "grsupra", "GR Supra")
                    +carsFiltersItem("cars-and-minivan", "sienna", "Sienna")
                    +carsFiltersItem("cars-and-minivan", "toyotacrown", "Toyota Crown")
                    +carsFiltersItem("cars-and-minivan", "yaris", "Yaris")
                    +carsFiltersItem("cars-and-minivan", "yarishatchback", "Yaris Hatchback")
                    +carsFiltersItem("cars-and-minivan", "yarisia", "Yaris iA")
                .tab-content.js-subtab-content#trucks
