section.section-middle
  .container
    .filters
      .filters-header
        h1.car-model__title Search RAV4 Inventory
        .filters-box
          .filters-box__group
            label.filters-box__label Search Radius:
            .select__wrap
              +icon("location", "select-icon-location")
              +icon("caret-down", "select-icon-caret-down")
              select.select.select2
                option Any
                option 10 Miles
                option 25 Miles
                option 50 Miles
                option 75 Miles
                option 100 Miles
                option 150 Miles
                option 250 Miles
                option 500 Miles
          .filters-box__group
            label.filters-box__label Zip Code:
            input.field(type="number")
      .tabs 
        .tabs-nav 
          button.tab-nav__btn.btn.btn--dropdown(data-tab="#vehicles") Vehicles
          button.tab-nav__btn.btn.btn--dropdown(data-tab="#price") Price
          button.tab-nav__btn.btn.btn--dropdown(data-tab="#year") Year
          button.tab-nav__btn.btn.btn--dropdown(data-tab="#mileage") Mileage
          button.tab-nav__btn.btn.btn--dropdown(data-tab="#color") Color
          button.tab-nav__btn.btn.btn--dropdown(data-tab="#drivetrain") Drivetrain
          button.tab-nav__btn.btn.btn--dropdown(data-tab="#dealer") Dealer
        .tabs-content
          .tab-content#vehicles 777
          .tab-content#price 1111
          .tab-content#year 222
          .tab-content#mileage 333
          .tab-content#color 444
          .tab-content#drivetrain 555
          .tab-content#dealer 6666
      .filters-content