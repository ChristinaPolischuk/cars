                    +carsFiltersItem("crossovers-and-suvs", "chr", "C-HR")
                    +carsFiltersItem("crossovers-and-suvs", "corollacross", "Corolla Cross")
                    +carsFiltersItem("crossovers-and-suvs", "fjcruiser", "FJ Cruiser")
                    +carsFiltersItem("crossovers-and-suvs", "highlander", "Highlander")
