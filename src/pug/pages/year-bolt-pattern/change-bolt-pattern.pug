            a.wheel-adapters-list__link.js-smooth-scroll(href="#adapter-1") 5x114.3 to 5x112
            a.wheel-adapters-list__link.js-smooth-scroll(href="#adapter-2") 5x114.3 to 5x120.70
            a.wheel-adapters-list__link.js-smooth-scroll(href="#adapter-3") 5x114.3 to 5x100
            a.wheel-adapters-list__link.js-smooth-scroll(href="#adapter-4") 5x114.3 to 5x108
            a.wheel-adapters-list__link.js-smooth-scroll(href="#adapter-5") 5x114.3 to 5x127
            a.wheel-adapters-list__link.js-smooth-scroll(href="#adapter-6") 5x114.3 to 5x120
            a.wheel-adapters-list__link.js-smooth-scroll(href="#adapter-7") 5x114.3 to 5x135
            a.wheel-adapters-list__link.js-smooth-scroll(href="#adapter-8") 5x114.3 to 5x139.7